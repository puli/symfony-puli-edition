{
    "version": "1.0",
    "name": "puli/symfony-puli-edition",
    "path-mappings": {
        "/acme/demo-bundle": "src/Acme/DemoBundle/Resources",
        "/acme/demo-bundle/controller": "src/Acme/DemoBundle/Controller",
        "/app": "app/Resources",
        "/app/config": "app/config"
    },
    "bindings": {
        "ee519a8a-4dd3-4104-8cb5-a56274c6acaf": {
            "_class": "Puli\\Discovery\\Binding\\ResourceBinding",
            "query": "/acme/demo-bundle/public{,/**/*}",
            "type": "puli/public-resource",
            "parameters": {
                "path": "/demo",
                "server": "localhost"
            }
        }
    },
    "config": {
        "bootstrap-file": "vendor/autoload.php"
    },
    "extra": {
        "servers": {
            "localhost": {
                "installer": "symlink",
                "document-root": "web"
            }
        }
    },
